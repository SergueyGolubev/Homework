    "df_master_entered = df_master.NAME[(df_master.PERIODE_PEDAGO=='Master semestre 1')| \n",
    "                                  (df_master.PERIODE_PEDAGO=='Master semestre 2')]\n",
    "df_master_passed = df_master.NAME[(df_master.PERIODE_PEDAGO=='Projet Master automne') | \n",
    "                                  (df_master.PERIODE_PEDAGO=='Projet Master printemps')]\n",
    "\n",
    "idx_entered = pd.Index(df_master_entered)\n",
    "idx_passed = pd.Index(df_master_passed)\n",
    "\n",
    "idx_res = idx_entered.intersection(idx_passed)\n",
    "df_master_passed = df_master[df_master.NAME.isin(idx_res)]\n",
    "df_master_passed.NAME.nunique()"
   ]
