   "source": [
    "df_entered = df.NAME[(df.PERIODE_PEDAGO=='Bachelor semestre 1')]\n",
    "df_passed = df.NAME[(df.PERIODE_PEDAGO=='Bachelor semestre 6')]\n",
    "\n",
    "idx_entered = pd.Index(df_entered)\n",
    "idx_passed = pd.Index(df_passed)\n",
    "\n",
    "idx_res = idx_entered.intersection(idx_passed)\n",
    "df_passed = df[df.NAME.isin(idx_res)]\n",
    "df_passed.NAME.nunique()"
   ]
