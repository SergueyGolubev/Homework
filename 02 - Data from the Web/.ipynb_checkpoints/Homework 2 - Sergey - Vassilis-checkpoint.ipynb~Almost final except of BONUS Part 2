    "%matplotlib inline\n",
    "import pandas as pd\n",
    "import glob, os\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.stats import mannwhitneyu\n",
    "\n",
    "pd.options.mode.chained_assignment = None  # default='warn'\n",
    "\n",
    "plt.rcParams[\"figure.figsize\"] = (10, 10)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "path = r'Data/'\n",
    "files = glob.glob(os.path.join(path, \"*Bachelor*.xls\"))\n",
    "dflist = []\n",
    "for file in files:\n",
    "    tmp = pd.read_html(file, skiprows=3)\n",
    "    if len(tmp)>0:\n",
    "        tmp = tmp[0]\n",
    "        tmp['PERIODE_ACAD'] = [acad_period for acad_period in acad_periods if file.find(acad_period)>-1][0]\n",
    "        tmp['PERIODE_PEDAGO'] = [pedag_period for pedag_period in pedag_periods if file.find(pedag_period)>-1][0]\n",
    "        dflist.append(tmp)\n",
    "df = pd.concat(dflist)\n",
    "df.dropna(axis=0, subset=[1], inplace=True)\n",
    "df.dropna(axis=1, inplace=True)\n",
    "df.columns = ['SEX', 'NAME', 'STATUS', 'SCIPER', 'PERIODE_ACAD', 'PERIODE_PEDAGO']\n",
    "df = df[((df.SEX=='Monsieur')|(df.SEX=='Madame'))]"
   ]
  },
